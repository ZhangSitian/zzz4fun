<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context-4.0.xsd">

    <context:annotation-config/>
    <bean id="sonnet29" class="com.zzz.learn.domain.poem.Sonnet29"/>
    <bean class="com.zzz.learn.domain.poem.Sonnet29"/>
    <bean id="duke" class="com.zzz.learn.domain.performer.PoeticJuggler"/>
    <bean id="kenny" class="com.zzz.learn.domain.performer.Instrumentalist">
        <property name="song"  value="Jingle Bells"/>
    </bean>

</beans>

